package Ejer2Profe.Model.Manejadores;

import Ejer2Profe.Model.Figuras.FiguraGeometrica;

public class ManejadorCuadrados extends ManejadorFiguras{

    public double areaTotal(){
        double suma = 0;
        for (FiguraGeometrica fig: figuras){
            suma += fig.calcularArea();
        }
        return suma;
    }

}
